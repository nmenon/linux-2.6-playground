		};

		timer16: timer@4882e000 {
			compatible = "ti,omap5430-timer";
			reg = <0x4882e000 0x80>;
			interrupts = <GIC_SPI 502 IRQ_TYPE_LEVEL_HIGH>;
			ti,hwmods = "timer16";
